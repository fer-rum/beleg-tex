\begin{tikzpicture}
\draw[->] (0, 1.25) rectangle (0.75, 0.25);
\draw (0.325, 0.75) node {A};
 
\draw (0, -0.25) rectangle (0.75, -1.25);
\draw (0.325, -0.75) node {B};

\draw[->] (0.75, 0.75)  -- (1, 0.75) -- (1, 0.05);
\draw[->] (0.75, -0.75) -- (1, -0.75) -- (1, -0.05);

\filldraw (1, 0) circle (0.05);

\draw[->] (1, 0) -- (1.5,  0);
\end{tikzpicture}